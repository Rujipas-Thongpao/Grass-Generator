#pragma kernel CSMain

struct GrassData {
    float3 position;
};

RWStructuredBuffer<GrassData> datas;

[numthreads(10,1,1)]
void CSMain (uint3 id : SV_DispatchThreadID)
{
    GrassData grass;
    grass.position = float3(id.x%10,1.0, id.x/10);
    grass.position -= float3(5.0,0.0,5.0);
    datas[id.x] = grass;
}
